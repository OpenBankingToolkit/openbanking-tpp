{
  "name": "forgerock-openbanking-ui",
  "project_version": "3.1.2-queen-rc6",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "test": "ng test --passWithNoTests",
    "test.watch": "ng test --watch",
    "test.ci": "ng test --ci --bail --passWithNoTests",
    "format:fix": "pretty-quick --staged",
    "precommit": "run-s format:fix",
    "lint": "ng lint",
    "=================TPP APP=================": "",
    "bundleSize.tpp": "openbanking-ui-cli appSize --theme forgerock --project tpp",
    "serve.tpp": "ng serve tpp",
    "serve.tpp.server": "cd ./projects/tpp/server && npm start",
    "report.tpp": "npm run build.tpp.forgerock && webpack-bundle-analyzer dist/forgerock/stats.json",
    "build.tpp.forgerock": "ng build --project=tpp --prod --configuration=forgerock --output-path dist/forgerock --statsJson --extra-webpack-config webpack.extra.js",
    "build.tpp.themes": "openbanking-ui-cli buildThemes --project tpp",
    "=================CDR-TPP APP=================": "",
    "build.cdr-tpp.server": "cd ./projects/cdr-tpp/server && npm run build",
    "serve.cdr-tpp.server": "cd ./projects/cdr-tpp/server && npm start",
    "build.cdr-tpp.compose": "ng build --project=cdr-tpp --prod --output-path dist/cdr-tpp --browserTarget cdr-tpp:build:compose",
    "=================CLUSTER UI-INTEG=================": "",
    "serve.ui-integ.tpp": "run-p serve.ui-integ.tpp.client serve.tpp.server",
    "serve.ui-integ.tpp.client": "ng serve tpp --host dev.tpp.ui-integ.forgerock.financial --browserTarget tpp:build:ui-integ --sslCert ssl/all.ui-integ.forgerock.financial.crt --sslKey ssl/k-all.ui-integ.forgerock.financial.key",
    "=================CLUSTER CDR=================": "",
    "serve.cdr.tpp": "run-p serve.cdr.tpp.client serve.tpp.server",
    "serve.cdr.tpp.client": "ng serve cdr-tpp --host 127.0.0.1 --browserTarget cdr-tpp:build:cdr",
    "serve.compose.cdr.tpp.client": "ng serve cdr-tpp --host 0.0.0.0 --browserTarget cdr-tpp:build:compose --ssl false",
    "serve.compose.cdr": "run-p serve.compose.cdr.tpp.client serve.cdr-tpp.server",
    "start.compose": "npm run serve.compose.cdr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.0",
    "@angular/cdk": "^9.0.0",
    "@angular/common": "^9.0.0",
